  </MSG_COMBATREVEALHANGING>

  <MSG_COMBATFLEE>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATFLEE</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing the participants that someone has fled.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatFlee</_MsgHandler>
      <ParticipantID TYPE="GID"></ParticipantID>
    </RECORD>
  </MSG_COMBATFLEE>

