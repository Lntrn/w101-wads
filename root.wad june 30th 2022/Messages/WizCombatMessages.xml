    </RECORD>
  </MSG_COMBATMATCHRESULT>

  <MSG_COMBATREVEALHANGING>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATREVEALHANGING</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing clients that a cloaked hanging effect has been revealed.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatRevealHanging</_MsgHandler>
