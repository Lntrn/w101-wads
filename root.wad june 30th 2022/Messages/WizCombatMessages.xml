      <ParticipantID TYPE="GID"></ParticipantID>
      <MoveType TYPE="UBYT"></MoveType>
      <SpellID TYPE="INT"></SpellID>
      <SpellTargetIndex TYPE="UINT"></SpellTargetIndex>
      <EnchantmentID TYPE="INT"></EnchantmentID>
      <IsItemCard TYPE="UBYT"></IsItemCard>
      <IsTreasureCard TYPE="UBYT"></IsTreasureCard>
      <IsBattleCard TYPE="UBYT"></IsBattleCard>
      </RECORD>
  </MSG_COMBATMOVESELECTION>

  <MSG_COMBATDRAW>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATDRAW</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">A client is requesting a card to be drawn from his sideboard. (Treasure Card Vault)</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatDraw</_MsgHandler>
    </RECORD>
  </MSG_COMBATDRAW>

  <MSG_COMBATPIPS>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATPIPS</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client the number of pips/powerpips each combat participant has in a particular duel.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatPips</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
      <PipData TYPE="STR"></PipData>
    </RECORD>
  </MSG_COMBATPIPS>

  <MSG_COMBATHEALTH>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATHEALTH</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client the health each combat participant has in a particular duel.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatHealth</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
      <HealthData TYPE="STR"></HealthData>
    </RECORD>
  </MSG_COMBATHEALTH>

  <MSG_COMBATSTATS>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATSTATS</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client the stats each combat participant has in a particular duel.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatStats</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
