<?xml version="1.0" ?>
<WizCombatMessages>

<_ProtocolInfo>
    <RECORD>
      <ServiceID TYPE="UBYT">51</ServiceID>
      <ProtocolType TYPE="STR">DOODLEDOUG_MESSAGES</ProtocolType>
      <ProtocolVersion TYPE="INT">1</ProtocolVersion>
      <ProtocolDescription TYPE="STR">Messages for all DoodleDoug minigames</ProtocolDescription>
    </RECORD>
  </_ProtocolInfo>

  <MSG_DUEL>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_DUEL</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">Informing a client of the contents of a Duel via Serialization.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_Duel</_MsgHandler>
      <Data TYPE="STR"></Data>
    </RECORD>
  </MSG_DUEL>

  <MSG_ENDDUEL>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_ENDDUEL</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">Informing a client to destroy and remove a Duel.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_EndDuel</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
    </RECORD>
  </MSG_ENDDUEL>

   <MSG_ALLOWLEAVEPVP>
      <RECORD>
         <_MsgName TYPE="STR" NOXFER="TRUE">MSG_ALLOWLEAVEPVP</_MsgName>
         <_MsgDescription TYPE="STR" NOXFER="TRUE">Informing a client to allow a player to leave a duel before it's destroyed.</_MsgDescription>
         <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_AllowLeavePvP</_MsgHandler>
      </RECORD>
   </MSG_ALLOWLEAVEPVP>

   <MSG_COMBATPHASE>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATPHASE</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server telling clients the phase of combat has changed.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatPhase</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
      <NewPhase TYPE="UBYT"></NewPhase>
      <Data TYPE="STR"></Data>
      <PlayerID TYPE="GID"></PlayerID>
    </RECORD>
  </MSG_COMBATPHASE>

  <MSG_COMBATMOVE>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATMOVE</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The client selecting a move of combat.  MoveType can be 0 for pass, 1 for attack, 2 for cast on a spell, 3 for flee.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatMove</_MsgHandler>
      <MoveType TYPE="UBYT"></MoveType>
      <SpellSelection TYPE="UBYT"></SpellSelection>
      <SpellTarget TYPE="UINT"></SpellTarget>
      <TimeLeft TYPE="INT"></TimeLeft>
    </RECORD>
  </MSG_COMBATMOVE>

  <MSG_COMBATHAND>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATHAND</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client of the cards in his hand, the number remaining in the deck, and the total number including graveyard (discarded or used cards).</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatHand</_MsgHandler>
      <HandData TYPE="STR"></HandData>
      <DeckCount TYPE="USHRT"></DeckCount>
      <TotalDeckCount TYPE="USHRT"></TotalDeckCount>
      <TreasureCardCount TYPE="USHRT"></TreasureCardCount>
      <ParticipantID TYPE="GID"></ParticipantID>
    </RECORD>
  </MSG_COMBATHAND>

  <MSG_COMBATACTIONS>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATACTIONS</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client of all the actions that have taken place this round.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatActions</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
      <ActionData TYPE="STR"></ActionData>
    </RECORD>
  </MSG_COMBATACTIONS>

  <MSG_COMBATADD>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATADD</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client that a new participant has joined the fray.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatAdd</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
      <ParticipantData TYPE="STR"></ParticipantData>
    </RECORD>
  </MSG_COMBATADD>

  <MSG_COMBATREMOVE>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATREMOVE</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The server informing a client that a participant has fled the battle.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatRemove</_MsgHandler>
      <DuelID TYPE="GID"></DuelID>
      <ParticipantID TYPE="GID"></ParticipantID>
    </RECORD>
  </MSG_COMBATREMOVE>

  <MSG_COMBATMOVESELECTION>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATMOVESELECTION</_MsgName>
