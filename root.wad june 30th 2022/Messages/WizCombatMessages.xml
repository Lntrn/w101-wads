    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATVICTORY</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">The client informing the server that it is done with a match.</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_CombatVictory</_MsgHandler>
    </RECORD>
  </MSG_COMBATVICTORY>

  <MSG_COMBATMATCHRESULT>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_COMBATMATCHRESULT</_MsgName>
