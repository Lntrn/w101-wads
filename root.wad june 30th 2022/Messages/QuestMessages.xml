<?xml version="1.0" ?>
<GameMessages>
  <_ProtocolInfo>
    <RECORD>
      <ServiceID TYPE="UBYT">52</ServiceID>
      <ProtocolType TYPE="STR">QUEST_MESSAGES</ProtocolType>
      <ProtocolVersion TYPE="INT">1</ProtocolVersion>
      <ProtocolDescription TYPE="STR">Messages related to questing</ProtocolDescription>
    </RECORD>
  </_ProtocolInfo>
  <MSG_SENDQUEST>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_SENDQUEST</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">Info for a given quest</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_SendQuest</_MsgHandler>
      <QuestID TYPE="GID"></QuestID>
      <QuestNameID TYPE="UINT"></QuestNameID>
      <QuestType TYPE="UINT"></QuestType>
	  <QuestLevel TYPE="INT"></QuestLevel>
      <QuestTitle TYPE="STR"></QuestTitle>
      <QuestInfo TYPE="STR"></QuestInfo>
      <New TYPE="UBYT"></New>
      <QuestMadlibs TYPE="STR"></QuestMadlibs>
      <GoalData TYPE="STR"></GoalData>
      <Rewards TYPE="STR"></Rewards>
      <ClientTags TYPE="STR"></ClientTags>
	  <AssociatedWorlds TYPE="STR"></AssociatedWorlds>
      <NoQuestHelper TYPE="UBYT"></NoQuestHelper>
      <Mainline TYPE="UBYT"></Mainline>
	  <ReadyToTurnIn TYPE="UBYT"></ReadyToTurnIn>
   	  <SkipQHAutoSelect TYPE="UBYT"></SkipQHAutoSelect>
      <PetOnlyQuest TYPE="UBYT"></PetOnlyQuest>
	  <ActivityType TYPE="UBYT"></ActivityType>
	</RECORD>
  </MSG_SENDQUEST>
  <MSG_SENDGOAL>
    <RECORD>
      <_MsgName TYPE="STR" NOXFER="TRUE">MSG_SENDGOAL</_MsgName>
      <_MsgDescription TYPE="STR" NOXFER="TRUE">Info for a given goal</_MsgDescription>
      <_MsgHandler TYPE="STR" NOXFER="TRUE">MSG_SendGoal</_MsgHandler>
      <QuestID TYPE="GID"></QuestID>
      <GoalID TYPE="GID"></GoalID>
      <GoalNameID TYPE="UINT"></GoalNameID>
      <GoalTitle TYPE="STR"></GoalTitle>
      <GoalLocation TYPE="STR"></GoalLocation>
      <GoalDestinationZone TYPE="STR"></GoalDestinationZone>
      <GoalImage1 TYPE="STR"></GoalImage1>
      <GoalImage2 TYPE="STR"></GoalImage2>
      <PersonaName TYPE="STR"></PersonaName>
      <GoalType TYPE="UBYT"></GoalType>
      <GoalStatus TYPE="UBYT"></GoalStatus>
      <GoalCount TYPE="INT"></GoalCount>
      <GoalTotal TYPE="INT"></GoalTotal>
      <SubscriberGoalTotal TYPE="INT"></SubscriberGoalTotal>
      <UseTally TYPE="UBYT"></UseTally>
      <TallyText TYPE="STR"></TallyText>
      <SendType TYPE="INT"></SendType>
      <GoalMadlibs TYPE="STR"></GoalMadlibs>
      <ClientTags TYPE="STR"></ClientTags>
      <PatronIcon TYPE="STR"></PatronIcon>
